    }
  },
  "00-fr-univ-lorr": {
    "fullName": "Université de Lorraine",
    "country": "France",
    "headers": {
      "Log-Format-ezproxy": "%h %{session}<[a-zA-Z0-9\\-\\+]+> %u %t \"%r\" %s %b %{user}<[a-zA-Z0-9\\-\\+,]+>",
